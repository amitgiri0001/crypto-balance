{
  "name": "hodlnaut-app",
  "version": "0.0.1",
  "description": "An app to see the investment profile.",
  "engines": {
    "node": ">= 16.13 <17"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.0.0",
    "@types/node": "~16.11.6",
    "@types/sinon": "^10.0.6",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.6.0",
    "eslint": "~8.1.0",
    "eslint-config-prettier": "~8.3.0",
    "eslint-plugin-mocha": "^10.0.1",
    "mocha": "^9.1.3",
    "nyc": "^15.1.0",
    "prettier": "~2.4.1",
    "rimraf": "~3.0.2",
    "sinon": "^12.0.1",
    "supertest": "^6.1.6",
    "ts-node": "^10.4.0",
    "tsutils": "~3.21.0",
    "typescript": "~4.4.4"
  },
  "scripts": {
    "start": "node build/server.js",
    "dev": "npm run clean && npm run build && NODE_ENV=development npm start",
    "clean": "rimraf coverage build tmp",
    "build": "npm run clean && tsc -p tsconfig.release.json",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "lint": "eslint . --ext .ts",
    "pretest": "npm run build",
    "test": "env NODE_ENV=testing TZ='utc' npm run coverage mocha --allow-console-logs \"build/__tests__\"",
    "test:watch": "mocha -r ts-node/register --allow-console-logs --watch \"build/__tests__\"",
    "posttest": "npm run lint",
    "coverage": "nyc"
  },
  "author": "Amit Giri",
  "dependencies": {
    "axios": "^0.24.0",
    "chai": "^4.3.4",
    "express": "^4.17.1",
    "http-errors": "^1.8.1",
    "tslib": "~2.3.1"
  }
}
